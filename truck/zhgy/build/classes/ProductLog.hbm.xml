<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.tornado.entity">
	<class
		name="ProductLog"
		table="PRODUCT_LOG"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="PRODUCT_LOG_ID"
		>
			<generator class="sequence"/>
		</id>

		<property
			name="ProductId"
			column="PRODUCT_ID"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="Name"
			column="NAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="Number"
			column="NUMBER"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Specifications"
			column="SPECIFICATIONS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Materials"
			column="MATERIALS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Processes"
			column="PROCESSES"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Packaging"
			column="PACKAGING"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Introduction"
			column="INTRODUCTION"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="Categoryid"
			column="CATEGORYID"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="ModifyDate"
			column="MODIFY_DATE"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="ModifyType"
			column="MODIFY_TYPE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ModifyBy"
			column="MODIFY_BY"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>